{
  "name": "react-promise",
  "version": "1.1.1",
  "description": "react.js component for dealing with promises",
  "main": "./lib/async.js",
  "jspm": {
    "format": "esm"
  },
  "scripts": {
    "test": "node run-tests",
    "pretest": "node_modules/.bin/standard",
    "precommit": "npm test",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development webpack src/async.js dist/async.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/async.js dist/async.min.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "clean": "rimraf lib dist",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capaj/react-async.git"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "keywords": [
    "react",
    "promise",
    "q"
  ],
  "author": "capajj@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/capaj/react-async/issues"
  },
  "homepage": "https://github.com/capaj/react-async#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "cross-env": "^5.0.0",
    "enzyme": "^2.8.2",
    "husky": "^0.13.3",
    "jsdom": "^10.1.0",
    "mocha": "^3.3.0",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.5.4",
    "standard": "^10.0.2",
    "webpack": "^2.5.1"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  }
}
